{
  "ci": {
    "collect": {
      "url": [
        "https://www.wikipedia.org",
        "https://www.cnn.com"
      ],
      "numberOfRuns": 1,
      "settings": {
        "chromeFlags": "--no-sandbox"
      }
    },
    "assert": {
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./lhci-reports"
    }
  }
}
